{
  "name": "uswds-sandbox",
  "private": true,
  "author": "18F",
  "license": "CC0-1.0",
  "scripts": {
    "federalist": "./build.sh && npm run uswds-copy-assets && npm run uswds-build-sass",
    "jekyll-install": "gem install bundler && bundle install",
    "serve": "bundle exec jekyll serve --livereload",
    "startup": "npm install && npm update uswds && ./build.sh && npm run jekyll-install && npm run uswds-copy-assets && npm run uswds-copy-theme && npm run uswds-build-sass",
    "uswds-build-sass": "gulp build-sass",
    "uswds-build-app": "gulp build-app",
    "uswds-copy-assets": "npm run uswds-copy-js && npm run uswds-copy-fonts && npm run uswds-copy-img",
    "uswds-copy-fonts": "gulp copy-uswds-fonts",
    "uswds-copy-img": "gulp copy-uswds-images",
    "uswds-copy-js": "gulp copy-uswds-js",
    "uswds-copy-theme": "gulp copy-uswds-setup",
    "uswds-update": "npm update uswds && ./build.sh",
    "watch": "gulp watch"
  },
  "dependencies": {
    "autoprefixer": "^8.6.5",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "^16.2.3",
    "chownr": "^1.1.1",
    "css-loader": "^0.28.7",
    "css-mqpacker": "^6.0.2",
    "cssnano": "^3.10.0",
    "del": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "gulp": "^4.0.0",
    "gulp-gzip": "^1.4.2",
    "gulp-notify": "^3.2.0",
    "gulp-postcss": "^7.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^0.6.1",
    "gulp-sass": "^3.2.1",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-watch": "^5.0.0",
    "path": "^0.12.7",
    "postcss": "^6.0.14",
    "postcss-loader": "^2.0.8",
    "postcss-uncss": "^0.16.1",
    "style-loader": "^0.20.3",
    "uncss": "^0.16.2",
    "url-loader": "^1.0.1",
    "uswds": "^2.0.0-beta.4",
    "uswds-gulp": "github:uswds/uswds-gulp",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "ajv": "^6.5.2"
  }
}
