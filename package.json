{
  "name": "uswds-sandbox",
  "private": true,
  "author": "18F",
  "license": "CC0-1.0",
  "scripts": {
    "dev": "webpack --progress --hide-modules",
    "watch": "webpack --watch --progress --hide-modules",
    "prod": "webpack --no-progress --hide-modules",
    "prepare": "npm run federalist",
    "federalist": "./build.sh && npm run uswds-assets && npm run prod",
    "uswds-install": "npm explore uswds -- npm install --only=dev",
    "uswds-update": "npm update uswds && npm run uswds-install && npm run uswds-build",
    "uswds-build": "npm explore uswds -- npm run build",
    "uswds-assets": "npm run uswds-mkdir && npm run uswds-fonts && npm run uswds-img",
    "uswds-fonts": "cp -R ./node_modules/uswds/dist/fonts/* ./assets/fonts/",
    "uswds-img": "cp -R ./node_modules/uswds/dist/img/* ./assets/img/",
    "uswds-mkdir": "mkdir -p assets/fonts/ && mkdir -p assets/img/"
  },
  "dependencies": {
    "autoprefixer": "^8.3.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.7",
    "css-mqpacker": "^6.0.2",
    "cssnano": "^3.10.0",
    "del": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "gulp": "^3.9.1",
    "gulp-gzip": "^1.4.2",
    "gulp-postcss": "^7.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-size": "^3.0.0",
    "gulp-watch": "^5.0.0",
    "path": "^0.12.7",
    "postcss": "^6.0.14",
    "postcss-loader": "^2.0.8",
    "postcss-uncss": "^0.16.1",
    "uncss": "^0.16.2",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "uswds": "uswds/uswds#dw-utilities-sandbox",
    "webpack": "^3.8.1"
  }
}
